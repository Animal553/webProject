<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qianfeng.mapper.EducationMapper">

    <select id="getEducationList" resultType="education">
        SELECT * FROM education
    </select>

    <select id="getEducation" parameterType="int" resultType="education">
        SELECT * FROM education WHERE education_id =#{educationId}
    </select>
</mapper>